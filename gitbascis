git init

git status

git log

>all files into staing
git add .
git add file_name

>commit
git commit -m 'message'

git commit -a -m 'direct commit'

git diff 

#create branch 
git checkout -b branch_name

#delete branch
git checkout -d branch_name

#to go into the branch_name below
git checkout branch_name

#stay in that branch in which u want to merge 
git merge branch_name

